@model Banding.Core.Models.Entities.MySql.Usuario

@{
    ViewData["Title"] = "Details";
}

<div class="container-xxl hero-header" style="padding-bottom:0px">
    <div class="container">


        <h1>Detalles</h1>

        <div>
            <h4>Usuario</h4>
            <hr />
            <dl class="row">
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.IdProvincia)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.IdProvincia)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.NombreUsuario)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.NombreUsuario)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.ApellidoUsuario)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.ApellidoUsuario)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.Celular)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.Celular)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.EMail)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.EMail)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.Username)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.Username)
                </dd>
                <dt class = "col-sm-2">
                    @Html.DisplayNameFor(model => model.Contrasena)
                </dt>
                <dd class = "col-sm-10">
                    @Html.DisplayFor(model => model.Contrasena)
                </dd>
            </dl>
        </div>
        <div>
            <a class="btn btn-dark" asp-action="Edit" asp-route-id="@Model.IdUsuario">Editar</a> |
            <a class="btn btn-dark" asp-action="Index">Regresar</a>
        </div>


    </div>
</div>